

<nav class="white" style="box-shadow:none;">
  <% if logged? %>
    <%= link_to 'Log out', session_destroy_path, method: :delete, :class=>"btn"%>
    <%= link_to 'Profile', user_profile_url(current_user.id), :class=>"btn", :style=>"margin-left:20px;" %>
  <% else %>
    <%= link_to 'Log in', login_path, :class=>"btn" %>
    <%= link_to 'Register', register_path, :class=>"btn", :style=>"margin-left:20px;" %>
  <% end %>
</nav>
